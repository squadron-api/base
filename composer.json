{
	"name": "squadron-api/base",
	"description": "Base Squadron package",
	"keywords": ["laravel", "squadron", "base", "api"],
	"license": "MIT",
	"authors": [{
		"name": "Andrey Novikov",
		"email": "andrew@7glyphs.com",
		"homepage": "https://7glyphs.com",
		"role": "Developer"
	}],
	"require": {
		"php": ">=7.2",
		"illuminate/console": "^5.8",
		"illuminate/database": "^5.8",
		"illuminate/support": "^5.8",
		"sentry/sentry-laravel": "^1.0"
	},
	"require-dev": {
		"orchestra/testbench": "~3",
		"phpunit/phpunit": "^8.0",
		"mockery/mockery": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Squadron\\Base\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Squadron\\Base\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Squadron\\Base\\ServiceProvider"
			]
		}
	}
}
